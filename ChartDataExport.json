{
  "id": "com.rohitchouhan.cde",
  "version": "1.0.0",
  "name": "Chart Data Export",
  "description": "A widget to export chart data such as dimensions, filter and values.",
  "newInstancePrefix": "ChartDataExport",
  "vendor": "Deloitte | rohchouhan@deloitte.com",
